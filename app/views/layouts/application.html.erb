    <meta name="viewport" content="width=device-width">
    <title><%= if content_for?(:titulo) then yield(:titulo) + ' | ' end %>
      Foie</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name='description'
      content='<%= yield_meta_tag(:description, 'Todas las recetas en una sola pagina web, Subi tus recetas y compartilas para que todos la vean, o simplemente busca la receta que queres!!') %>' />
    <meta name='keywords'
      content='<%= yield_meta_tag(:keywords, 'recetas,receta,foie,comida,rico,delicioso,como,hacer,cocina,trucos,ingredientes,pasos,defaults') %>' />

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
